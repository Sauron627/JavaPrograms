import java.io.IOException;


public class Runner {

	public static void main(String[] args) throws IOException {
		
		LakoTelepReader.read("input.txt");
		lakotelep ltp = new lakotelep(LakoTelepReader.tomb);
		ltp.writeSzintEloszlas();
		System.out.printf("Körpanorámás házak száma: %d",ltp.getKorPanoramasHazakSzama());
		
	}

}
